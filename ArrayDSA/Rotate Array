class Solution {
public:
    void rotate(vector<int>& nums, int k) {
    int n = nums.size();
    if(n==0 || n==1)
    return ;
    if(k>n){
        k = k%n;
    }
    reverse(nums.begin(),nums.end());
    reverse(nums.begin(),nums.begin()+k); //reverse k elements
    reverse(nums.begin()+k,nums.end()); //reverse n-k elements
    }
};
