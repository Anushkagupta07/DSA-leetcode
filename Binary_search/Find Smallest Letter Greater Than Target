class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
     int s = 0;
     int e = letters.size()-1;
     char res = '#';
     while(s<=e){
        int mid = s+(e-s)/2;
        if(letters[mid]<=target){
            s = mid+1;
        }else{
            res = letters[mid];
            e = mid-1;
        }
     }  
     if(res=='#') return letters[0];
     else return res;
    }
};
