class Solution {
  public:
    int findFloor(vector<int>& arr, int x) {
        int s = 0;
        int e = arr.size()-1;
        int res = -1;
        while(s<=e){
            int mid = s + (e-s)/2;
            if(arr[mid]<=x){
                res = mid;
                s = mid+1;
            }else{
                 e = mid-1;
            }
        }
        return res;
    }
};
